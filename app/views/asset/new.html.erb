<div class="container">
  <%= form_with url: asset_index_path do |form| %>
    <div class="form-group">
      <%= form.label :asset_type %>
      <%= form.number_field :asset_type, class: "form-control"%>
    </div>
    <div class="form-group">
      <%= form.label :name %>
      <%= form.number_field :name, class: "form-control"%>
    </div>
    <div class="form-group">
      <%= form.submit "保存", :class => "btn btn-info" %>
    </div>
  <% end %>
</div>