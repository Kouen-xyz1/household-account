<div class="container">
  <%= form_with model: @profit do |form| %>
    <div class="form-group">
      <%= form.label :category %>
      <%= form.number_field :category, class: "form-control"%>
    </div>
    <div class="form-group">
      <%= form.label :sub_category1 %>
      <%= form.number_field :sub_category1, class: "form-control"%>
    </div>
    <div class="form-group">
      <%= form.label :sub_category2 %>
      <%= form.number_field :sub_category2, class: "form-control"%>
    </div>
    <div class="form-group">
      <%= form.label :year_2019_01 %>
      <%= form.number_field :year_2019_01, class: "form-control"%>
    </div>
    <div class="form-group">
      <%= form.label :year_2019_02 %>
      <%= form.number_field :year_2019_02, class: "form-control"%>
    </div>
    <div class="form-group">
      <%= form.label :year_2019_03 %>
      <%= form.number_field :year_2019_03, class: "form-control"%>
    </div>
    <div class="form-group">
      <%= form.submit "保存", :class => "btn btn-info" %>
    </div>
  <% end %>
</div>